{
  "root": "packages/agent",
  "targets": {
    "build": {
      "dependsOn": ["^build"],
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/agent",
        "command": "tsc"
      }
    },
    "test": {
      "dependsOn": ["build"],
      "executor": "nx:run-commands",
      "options": {
        "cwd": "lib/task",
        "command": "vitest run .test.ts"
      }
    }
  }
}
